{
  "name": "vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.11.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
    "start": "vite preview --host",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,vue}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,vue}",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,vue,css,md,json}' --config ./.prettierrc",
    "prepare": "husky"
  },
  "dependencies": {
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "dotenv": "^16.4.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.2",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.22.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5"
  }
}
